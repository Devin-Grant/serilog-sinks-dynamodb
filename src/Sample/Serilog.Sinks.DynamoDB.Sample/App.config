<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
    </startup>


  <appSettings>
    <add key="AWSRegion" value="ap-southeast-2" />
    <add key="AWSAccessKey" value="{My AWS Access Key}" />
    <add key="AWSSecretKey" value="{My AWS Secret Access Key}" />

  </appSettings>

  <system.net>
    <defaultProxy useDefaultCredentials="true">
    </defaultProxy>
  </system.net>

</configuration>